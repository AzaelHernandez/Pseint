Proceso Multiplo
	//crear un pseudocodigo que lea un numero entero y diga si es múltiplo de 3
	Definir num como Entero;
	Imprimir "Introduzca un número para comprobar es multiplo de 3";
	leer num;
	si (num%3) == 0 entonces
		Imprimir "el numero, ",num," es multiplo de 3";
	SiNo
		Imprimir "el numero, ",num," no es multiplo de 3";
	FinSi
	
FinProceso
