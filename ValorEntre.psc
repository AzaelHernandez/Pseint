Proceso ValorEntre
	//Crar un pseudocodigo que lea un numero entero y muestre si esta entre 10 y 20
	definir a Como Real;
	
	Imprimir "Introduce un valor entero";
	leer a;
	
	si a>=10 y a<=20 Entonces
		Imprimir "El valor ",a," se encuentra dentro de 10 y 20";
	SiNo
		Imprimir "El valor introducido no se encuentra dentro de 10 y 20";
	FinSi
	
	
FinProceso
