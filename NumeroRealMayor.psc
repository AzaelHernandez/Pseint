Proceso NumeroRealMayor
	//Crear un pseudocódigo que lea dos números reales e imprima el mayor de ellos
	
	definir a Como Real;
	definir b Como Real;
	
	Imprimir "Ingrese un número";
	leer a;
	
	Imprimir "Ingrese otro número";
	leer b;
	
	
	si a>b Entonces
		Imprimir "el valor de ",a," es mayor que ",b;
	SiNo
		Imprimir "el valor de ",b," es mayor que ",a;
	FinSi
FinProceso
